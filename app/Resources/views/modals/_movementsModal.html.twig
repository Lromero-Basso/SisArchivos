{# Modal devolver carpeta #}
<div class="modal fade" id="modalMovements" tabindex="-1" role="dialog" aria-labelledby="modalMovementsLabel" aria-hidden="true">
  <div class="modal-dialog" role="document">
    <form>
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title" id="modalMovementsLabel">Devolver carpeta</h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body">
                <div class="row">
                    <div class="col-md-4">
                        <label>Código carpeta<span class="ml-1 text-danger">*</span></label>
                        <input class="form-control" type="text" name="Histarch[codCarp]" value="" readonly>
                    </div>
                    <div class="col-md-4">
                        <label>Legajo<span class="ml-1 text-danger">*</span></label>
                        <input class="form-control" type="text" name="Histarch[legajo]" value="" readonly>
                    </div>
                </div>
                <br>
                <div class="row">
                    <div class="col-md-6">
                        <label>Fecha retiro<span class="ml-1 text-danger">*</span></label>
                          <input class="form-control" type="date" name="Histarch[fechaRetiro]" value="" readonly>
                    </div>
                    <div class="col-md-6">
                        <label>Fecha devolución<span class="ml-1 text-danger">*</span></label>
                        <input class="form-control" type="date" name="Histarch[fechaDevolucion]" value="" readonly>
                    </div>
                </div>
            </div>
            <div class="modal-footer">
                <a class="btn btn-secondary text-white" onclick="closeModal(modalMovements)"  style="margin-top:30px;"> 
                    <i class="fas fa-undo-alt"></i> Cerrar
                </a>
            </div>
        </div>
    </form>
  </div>
</div>